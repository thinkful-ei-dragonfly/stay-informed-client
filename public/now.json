{
   "version": 2,
   "name": "stay-informed",
   "routes": [
       {
         "src": "^/static/(.*)",
         "dest": "/static/$1"
       },
       { "src": "^/favicon.ico$",
         "dest": "/favicon.ico"
       },
       {
         "src": "^/manifest.json$",
         "dest": "/manifest.json"
        },
       {
         "src": ".*",
         "dest": "/index.html"
       }

   ]
 }
